{
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "name": "cargamento-service",
    "projectType": "application",
    "sourceRoot": "apps/api/cargamento-service/src",
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/apps/api/cargamento-service",
                "main": "apps/api/cargamento-service/src/main.ts",
                "tsConfig": "apps/api/cargamento-service/tsconfig.app.json",
                "assets": []
            }
        },
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "node dist/apps/api/cargamento-service/main.js"
            },
            "dependsOn": [
                "build"
            ]
        }
    },
    "tags": []
}